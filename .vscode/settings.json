{
    "editor.tabSize": 4,
    "editor.formatOnSave": true,
    "files.exclude": {
        "**/__pycache__": true,
        "**/.pytest_cache": true,
        "**/.DS_Store": true,
        "**/.hg": true,
        "**/.git": true,
        "**/*.pyc": true,
        "**/.svn": true,
        "**/Thumbs.db": true,
        "**/venv": true,
        "**/.venv": true,
    },
    "python.analysis.autoImportCompletions": true,
    "python.analysis.completeFunctionParens": true,
    "python.analysis.typeCheckingMode": "strict"
}